/* === Background === */
#centralwidget {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #f5f7fa, stop:1 #c3cfe2);
}

/* === Header === */
QFrame#headerFrame {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 #667eea, stop:1 #764ba2);
    border-radius: 15px;
    padding: 15px;
}
QLabel#titleLabel {
    color: white;
    font-size: 24pt;
    font-weight: bold;
    background: transparent;
}
QLabel#subtitleLabel {
    color: rgba(255, 255, 255, 0.95);
    font-size: 11pt;
    font-weight: normal;
    background: transparent;
}

/* === Group Boxes === */
QGroupBox {
    font-weight: bold;
    font-size: 12pt;
    color: #2c3e50;
    border: 2px solid #3498db;
    border-radius: 10px;
    margin-top: 15px;
    padding-top: 15px;
    background: white;
}
QGroupBox::title {
    subcontrol-origin: margin;
    left: 10px;
    padding: 0 10px;
}
QGroupBox#parametersGroupBox { border-color: #e74c3c; }
QGroupBox#jumpGroupBox { border-color: #f39c12; }
QGroupBox#actionsGroupBox { border-color: #27ae60; }

/* === Labels === */
QGroupBox QLabel {
    font-weight: bold;
    font-size: 10pt;
    color: #34495e;
    background: transparent;
}

/* === Line Edits === */
QLineEdit {
    font-size: 10pt;
    font-weight: bold;
    color: #2c3e50;
    padding: 6px;
    border: 2px solid #bdc3c7;
    border-radius: 6px;
    background: #ecf0f1;
}
QLineEdit:focus {
    color: #2c3e50;
    border-color: #3498db;
    background: white;
}
QLineEdit#lineEdit_n {
    font-size: 11pt;
    padding: 8px;
}
QLineEdit#lineEdit_n:focus {
    border-color: #f39c12;
}

/* === ComboBox === */
QComboBox {
    font-size: 11pt;
    color: #2c3e50;
    padding: 8px;
    border: 2px solid #bdc3c7;
    border-radius: 8px;
    background: white;
}
QComboBox QAbstractItemView {
    color: #2c3e50;
    background: white;
    selection-background-color: #3498db;
    selection-color: white;
}
QComboBox:hover { border-color: #3498db; }
QComboBox::drop-down { border: none; width: 30px; }
QComboBox::down-arrow {
    image: none;
    border-left: 5px solid transparent;
    border-right: 5px solid transparent;
    border-top: 8px solid #34495e;
    margin-right: 10px;
}

/* === Buttons — common === */
QPushButton {
    font-weight: bold;
    font-size: 10pt;
    color: white;
    border: none;
    border-radius: 8px;
    padding: 10px;
}

/* Blue — data input */
QPushButton#pushButton_inputData {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #3498db, stop:1 #2980b9);
}
QPushButton#pushButton_inputData:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #5dade2, stop:1 #3498db);
}
QPushButton#pushButton_inputData:pressed {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #2980b9, stop:1 #1f618d);
}

/* Purple — statistical tests */
QPushButton#pushButton_hinich, QPushButton#pushButton_segmentation {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #9b59b6, stop:1 #8e44ad);
}
QPushButton#pushButton_hinich:hover, QPushButton#pushButton_segmentation:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #bb8fce, stop:1 #9b59b6);
}
QPushButton#pushButton_hinich:pressed, QPushButton#pushButton_segmentation:pressed {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #8e44ad, stop:1 #6c3483);
}

/* Red — fractal analysis */
QPushButton#pushButton_herstLyapunov, QPushButton#pushButton_hoelder {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #e74c3c, stop:1 #c0392b);
}
QPushButton#pushButton_herstLyapunov:hover, QPushButton#pushButton_hoelder:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #ec7063, stop:1 #e74c3c);
}
QPushButton#pushButton_herstLyapunov:pressed, QPushButton#pushButton_hoelder:pressed {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #c0392b, stop:1 #922b21);
}

/* Green — kurtosis */
QPushButton#pushButton_curth {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #27ae60, stop:1 #229954);
}
QPushButton#pushButton_curth:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #58d68d, stop:1 #27ae60);
}
QPushButton#pushButton_curth:pressed {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #229954, stop:1 #1e8449);
}

/* Orange — bifurcation */
QPushButton#pushButton_bifurcation {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #f39c12, stop:1 #e67e22);
}
QPushButton#pushButton_bifurcation:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #f7dc6f, stop:1 #f39c12);
}
QPushButton#pushButton_bifurcation:pressed {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #e67e22, stop:1 #d35400);
}

/* Yellow-orange — spectrogram */
QPushButton#pushButton_spectrogram {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #f39c12, stop:1 #d68910);
}
QPushButton#pushButton_spectrogram:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #f5b041, stop:1 #f39c12);
}
QPushButton#pushButton_spectrogram:pressed {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #d68910, stop:1 #b9770e);
}

/* Teal — advanced analysis */
QPushButton#pushButton_cumulative, QPushButton#pushButton_oscillations {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #16a085, stop:1 #138d75);
}
QPushButton#pushButton_cumulative:hover, QPushButton#pushButton_oscillations:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #48c9b0, stop:1 #16a085);
}
QPushButton#pushButton_cumulative:pressed, QPushButton#pushButton_oscillations:pressed {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #138d75, stop:1 #0e6655);
}

/* Gray — exit */
QPushButton#pushButton_exit {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #95a5a6, stop:1 #7f8c8d);
}
QPushButton#pushButton_exit:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #b2babb, stop:1 #95a5a6);
}
QPushButton#pushButton_exit:pressed {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #7f8c8d, stop:1 #566573);
}
